{
  "version": 2,
  "builds": [
    {
      "src": "hakaseAI/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "hakaseAI/**",
      "use": "@vercel/static"
    },
    {
      "src": "poinavi/**",
      "use": "@vercel/static"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/hakaseAI/api/(.*)",
      "dest": "/hakaseAI/server.js"
    },
    {
      "src": "/hakaseAI$",
      "status": 308,
      "headers": { "Location": "/hakaseAI/" }
    },
    {
      "src": "/hakaseAI/",
      "dest": "/hakaseAI/index.html"
    },
    {
      "src": "/hakaseAI/(.*\\.(css|js|png|jpg|svg|json|ico))",
      "dest": "/hakaseAI/$1"
    },
    {
      "src": "/poinavi$",
      "status": 308,
      "headers": { "Location": "/poinavi/" }
    },
    {
      "src": "/poinavi/",
      "dest": "/poinavi/index.html"
    },
    {
      "src": "/poinavi/(.*)",
      "dest": "/poinavi/$1"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
